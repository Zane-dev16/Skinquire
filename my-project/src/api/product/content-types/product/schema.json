{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "rating": {
      "type": "integer",
      "min": 0,
      "max": 10,
      "default": 0,
      "required": true
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "ingredients": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ingredient.ingredient",
      "mappedBy": "products"
    }
  }
}
